<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xhq.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.xhq.sample.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.xhq.sample.MultiItemListViewActivity"
            android:label="@string/title_activity_multi_item_list_view">
        </activity>
        <activity
            android:name="com.xhq.sample.RecyclerViewActivity"
            android:label="@string/title_activity_recycler_view">
        </activity>
        <activity
            android:name="com.xhq.sample.MultiItemRvActivity"
            android:label="@string/title_activity_multi_item_rv">
        </activity>
    </application>

</manifest>
